#String template class

#Substitute value of x in above template
print(t.substitute({'x':1}))

print("hello in Japanese is %s",%"Konichiwa"))
print("hello in Japanese is {}",format("Konichiwa"))
print(f"when we add 12 and 24 we get{12+24}")

print("HELLO %s,welcome %s"%("Guys",To python"))
print("Hii my name is{}".format("Tom"))
a="Hello"
print(f"{a}world")

#List Student stores the name and marks of three students
Student=[('Ram',90),('Ankit',78),('Bob',92)]
#We are creating a basic structure to print the name and
#marks of the students.
t=template('Hii $name,you have got $marks marks,)
for i in Student:
print(t.substitute (name=i[0],marks=i[1]))
